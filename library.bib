journal = {Marine Policy},
keywords = {Armchair fishing,Catch shares,Fisheries Management Schemes,Individual transferable quotas,Small-scale fisheries},
pages = {76--81},
title = {{Using individual transferable quotas (ITQs) to achieve social policy objectives: A proposed intervention}},
url = {https://ac.els-cdn.com/S0308597X13002807/1-s2.0-S0308597X13002807-main.pdf?{\_}tid=bff1f984-ba7c-11e7-b06e-00000aab0f27{\&}acdnat=1509043242{\_}ac8c21fcb3d701166677e8904cf4ae2e},
