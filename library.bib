@article{Hannesson2005,
abstract = {The basic economic problem of commercial fisheries can be seen as{\$}\backslash{\$}nthe absence of property rights to the fish stocks. There are, however,{\$}\backslash{\$}nboth practical and principal obstacles to applying that solution.{\$}\backslash{\$}nInstead, the preferable arrangement appears to be stock control by{\$}\backslash{\$}npublic agencies, combined with exclusive use rights granted, leased{\$}\backslash{\$}nor sold to the fishing industry in order to achieve economic efficiency.{\$}\backslash{\$}nIncentives to establish such rights can be found both in government{\$}\backslash{\$}ncircles and in the industry. Both have a vital role to play; plans{\$}\backslash{\$}nto establish use rights such as ITQs have run aground on more than{\$}\backslash{\$}none occasion because of opposition from the industry. Use rights{\$}\backslash{\$}ncan be seen as a tool for government agencies to achieve economic{\$}\backslash{\$}nefficiency in the industry. If desired, these gains can be distributed{\$}\backslash{\$}nover a wider public through user fees of some kind. There are few{\$}\backslash{\$}nexamples, however, of substantial user fees, and the industry has{\$}\backslash{\$}nbeen successful in getting such schemes abolished when they have{\$}\backslash{\$}nbeen put into effect. The experience of Russia and, in particular,{\$}\backslash{\$}nEstonia is discussed. The absence of user fees can be explained by{\$}\backslash{\$}nthe need to obtain support for use right schemes from the industry,{\$}\backslash{\$}nand the windfall gains from gratis use rights are vehicles for this.{\$}\backslash{\$}nNorway and Iceland are two countries which have put in place use{\$}\backslash{\$}nrights, but they have done so in different ways. Iceland has an ITQ{\$}\backslash{\$}nsystem while Norway has fishing concessions combined with individual{\$}\backslash{\$}nvessel quotas. Some success appears to have been achieved in both{\$}\backslash{\$}ncountries with these systems.},
author = {Hannesson, R{\"{o}}gnvaldur},
doi = {10.1007/s11160-005-4870-3},
isbn = {4755959543},
issn = {09603166},
journal = {Reviews in Fish Biology and Fisheries},
keywords = {Fisheries economics,Fishing rights,ITQs},
