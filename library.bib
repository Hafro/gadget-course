@article{McCormack2017,
abstract = {New Zealand's fisheries management institutions represent a globally recognised story of a successful sustainable management regime, an accolade perceived to be based on its early and comprehensive adoption of a quota management system (QMS). This article questions these assumptions. There are three main strands to the argument. First, that the interpretation of sustainability in the New Zealand QMS disregards the social while simultaneously accentuating a particularly neoliberal economic paradigm in which sustainability is directed towards sustaining the wealth generating potential of quota holdings. Second, while in theory there is a separation of biological and economic conceptions of sustainability in the QMS, these processes are, in fact, deeply intertwined. Third, that the sustainability brand works to legitimise the privatisation and marketization of marine environments, to protect the income stream of quota investors, and to effectively incorporate and discipline dissent.},
author = {McCormack, Fiona},
doi = {10.1016/j.marpol.2016.06.022},
issn = {0308597X},
journal = {Marine Policy},
keywords = {Individual Transferable Quota,Māori,Neoliberalism,Sustainability},
pages = {35--46},
title = {{Sustainability in New Zealand's quota management system: A convenient story}},
url = {https://ac.els-cdn.com/S0308597X16303785/1-s2.0-S0308597X16303785-main.pdf?{\_}tid=5e17c508-c612-11e7-9e18-00000aacb35f{\&}acdnat=1510316966{\_}f48ad3884aaffec03fd562565cc87fc1},
volume = {80},
year = {2017}
}
@misc{Grafton2006,
abstract = {The failures of traditional target-species management have led many to propose an ecosystem approach to fisheries to promote sustainability. The ecosystem approach is necessary, especially to account for fishery-ecosystem interactions, but by itself is not sufficient to address two important factors contributing to unsustainable fisheries: inappropriate incentives bearing on fishers and the ineffective governance that frequently exists in commercial, developed fisheries managed primarily by total-harvest limits and input controls. We contend that much greater emphasis must be placed on fisher motivation when managing fisheries. Using evidence from more than a dozen natural experiments in commercial fisheries, we argue that incentive-based approaches that better specify community and individual harvest or territorial rights and price ecosystem services and that are coupled with public research, monitoring, and effective oversight promote sustainable fisheries.},
author = {Grafton, R Quentin and Arnason, Ragnar and Bj{\o}rndal, Trond and Campbell, David and Campbell, Harry F and Clark, Colin W and Connor, Robin and Dupont, Diane P and Hannesson, R{\"{o}}gnvaldur and Hilborn, Ray and Kirkley, James E and Kompas, Tom and Lane, Daniel E and Munro, Gordon R and Pascoe, Sean and Squires, Dale and Steinshamn, Stein Ivar and Turris, Bruce R and Weninger, Quinn},
booktitle = {Canadian Journal of Fisheries and Aquatic Sciences},
doi = {10.1139/f05-247},
isbn = {0706-652X},
issn = {0706-652X},
number = {3},
pages = {699--710},
title = {{Incentive-based approaches to sustainable fisheries}},
volume = {63},
year = {2006}
}
@article{Stewart2015,
abstract = {Balancing catch with annual catch entitlement (ACE) is crucial both for the financial viability of fishers and for the efficient operation of the New Zealand quota management system (QMS). This study examines the information channels that are used by fishers to search for ACE availability, and notes how these channels differ between large and small fishers. Special attention is given to the viability of small independent fishers whose participation in the fishery is dependent on their acquiring ACE in the open ACE market. An ACE market survey along with extensive consultation captured the views of fishers, fish processors, and quota brokers. These sources provide valuable insights into the day-to-day operation of the ACE market. They suggest that while elements of market power are evident in the ACE market there are also characteristics of an efficient market.},
author = {Stewart, James and Leaver, Jonathan},
doi = {10.1016/j.marpol.2015.01.005},
issn = {0308597X},
journal = {Marine Policy},
keywords = {Annual catch entitlement (ACE),Deemed value,Efficient markets,Information channels,Transaction cost},
pages = {11--22},
title = {{Efficiency of the New Zealand annual catch entitlement market}},
url = {http://www.sciencedirect.com/science/article/pii/S0308597X15000160},
volume = {55},
year = {2015}
}
