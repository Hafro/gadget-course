@article{Altonji1986,
abstract = {The sensitivity of the supply of labor to intertemporal variation{\$}\backslash{\$}nin the wage is an important issue in macroeconomics, the analysis{\$}\backslash{\$}nof social security and pensions, and the study of life-cycle patterns{\$}\backslash{\$}nof work. This paper explores two approaches to the measurement of{\$}\backslash{\$}nintertemporal substitution that have appeared in the literature.{\$}\backslash{\$}nThe first approach is to use consumption to control for wealth and{\$}\backslash{\$}nunobserved expectations about future wages in the labor supply equation.{\$}\backslash{\$}nThe second approach is to estimate a first-difference equation for{\$}\backslash{\$}nhours in which labor supply from the previous period serves as a{\$}\backslash{\$}ncontrol for wealth and wage expectations. The results indicate that{\$}\backslash{\$}nintertemporal substitution elasticity for married men is positive{\$}\backslash{\$}nbut small.},
