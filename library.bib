journal = {Ecological Economics},
keywords = {Biodiversity,Contribution value,Resource-based model,Stochastic viability},
number = {1-2},
pages = {14--23},
publisher = {Elsevier B.V.},
title = {{Contribution values of biodiversity to ecosystem performances: A viability perspective}},
url = {http://dx.doi.org/10.1016/j.ecolecon.2008.08.015},
volume = {68},
year = {2008}
}
