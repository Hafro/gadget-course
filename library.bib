keywords = {Activity,Dietary recommendation,Food choice attitudes,Food consumption survey,Motivation},
number = {1},
pmid = {26048166},
title = {{The creation of a healthy eating motivation score and its association with food choice and physical activity in a cross sectional sample of Irish adults}},
url = {https://ijbnpa.biomedcentral.com/track/pdf/10.1186/s12966-015-0234-0?site=ijbnpa.biomedcentral.com},
volume = {12},
year = {2015}
}
