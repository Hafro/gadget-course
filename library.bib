file = {:C$\backslash$:/Users/bthe/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Rademeyer, Plag{\'{a}}nyi, Butterworth - 2007 - Tips and tricks in designing management procedures(3).pdf:pdf},
