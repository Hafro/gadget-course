@article{Arnason1990,
abstract = {This paper deals with problems of optimal management of common-property{\$}\backslash{\$}nfisheries. It advances the proposition that many fisheries management{\$}\backslash{\$}nschemes, which are theoretically capable of generating efficiency,{\$}\backslash{\$}nare actually not practicable, owing to their huge informational requirements.{\$}\backslash{\$}nThis applies, for instance, to management by means of corrective{\$}\backslash{\$}ntaxes/subsidies. The paper proceeds to show that there exists, under{\$}\backslash{\$}nfairly unrestrictive conditions, market-based management systems{\$}\backslash{\$}nthat require minimal information for their operation but lead nevertheless{\$}\backslash{\$}nto efficiency in common-property fisheries. One such system is the{\$}\backslash{\$}nIndividual Transferable Share Quota system (ITSQ).},
