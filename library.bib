publisher = {Elsevier},
title = {{Multi-criteria decision-making for fisheries management: A case study of Mediterranean demersal fisheries}},
url = {https://ac.els-cdn.com/S0308597X1400298X/1-s2.0-S0308597X1400298X-main.pdf?{\_}tid=2364b39a-1643-11e8-9492-00000aacb35f{\&}acdnat=1519134005{\_}c957e86d35425ae04558b3555717347a},
