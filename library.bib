number = {11},
pages = {1056--1068},
title = {{A fuzzy-logic tool for multi-criteria decision making in fisheries: The case of the South African pelagic fishery}},
url = {https://www.researchgate.net/profile/Barbara{\_}Paterson2/publication/248887395{\_}A{\_}fuzzy-logic{\_}tool{\_}for{\_}multi-criteria{\_}decision{\_}making{\_}in{\_}fisheries{\_}The{\_}case{\_}of{\_}the{\_}South{\_}African{\_}pelagic{\_}fishery/links/553c2c680cf29b5ee4b882dc/A-fuzzy-logic-tool-for-multi-crite},
