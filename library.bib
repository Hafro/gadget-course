year = {2010}
}
@article{Arnason2004,
abstract = {The economic efficiencies of the Danish, Icelandic, and Norwegian{\$}\backslash{\$}ncod fisheries are examined. For this purpose, nonlinear aggregate{\$}\backslash{\$}nmodels of these fisheries are constructed. Comparing the calculated{\$}\backslash{\$}noptimal harvest and biomass quantities with the actual fisheries{\$}\backslash{\$}nprovides a measure of the degree of efficiency in these fisheries.{\$}\backslash{\$}nThe comparisons confirm that the cod harvesting policies of these{\$}\backslash{\$}ncountries have been hugely inefficient in the past. It appears that{\$}\backslash{\$}ninefficiency has been increasing over the last three to four decades,{\$}\backslash{\$}neven after TAC regulations replaced open access, indicating that{\$}\backslash{\$}nthe management policies adopted by all three countries have failed{\$}\backslash{\$}nto cure overfishing.},
