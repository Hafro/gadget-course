keywords = {cost of fisheries management,efficient fisheries,fisheries management,management,multi-objective fisheries management,operations research and},
pages = {157--179},
publisher = {Springer Science + Business media},
title = {{Chapter 9 FISHERIES MANAGEMENT}},
year = {2007}
}
