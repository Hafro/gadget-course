issn = {10543139},
journal = {ICES Journal of Marine Science},
keywords = {population dynamics,projections,stock assessment,uncertainty,yellowfin tuna},
number = {6},
pages = {969--979},
title = {{Including parameter uncertainty in forward projections of computationally intensive statistical population dynamic models}},
volume = {63},
year = {2006}
}
