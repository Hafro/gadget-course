@article{Baudron2010,
abstract = {Total allowable catch (TAC) management has in many fisheries, especially mixed fisheries, failed to meet conservation objectives. For instance, for the Faroe Plateau mixed demersal fisheries, the TAC system failed to achieve the objective of an average annual fishing mortality of 0.45 for the three gadoid stocks cod (Gadus morhua), haddock (Melanogrammus aeglefinus), and saithe (Pollachius virens). Therefore, in 1996, an effort-regulation system with individual transferable effort quotas was introduced to manage the fisheries. Experience has shown that effort management without additional stock-specific measures may not be appropriate for such fisheries. A management strategy evaluation model was developed to compare an effort-management system based on the Faroese example with a TAC system as currently applied in EU fisheries. Results show that when stocks are considered in isolation, a total allowable effort system does not necessarily perform better than a TAC one. It depends on stock status and dynamics, the level of uncertainty, and the reactivity of the system to changes in scientific advice. When the stocks are considered together in mixed fisheries, effort management seems, however, to be appropriate, and interannual flexibility of the system appears to be the best compromise between short-and long-term objectives, as well as between biological sustainability and economic return.},
author = {Baudron, Alan and Ulrich, Clara and {Rasmus Nielsen}, J and {Boje Baudron}, Jesper and Ulrich, C and Nielsen, J R and Boje, J and to Ulrich, Correspondence C},
journal = {Faroe Islands example. – ICES Journal of Marine Science},
keywords = {Faroe Islands,effort management,management strategy evaluation (MSE),mixed fisheries},
pages = {1036--1050},
title = {{Comparative evaluation of a mixed-fisheries effort-management system based on the Faroe Islands example Comparative evaluation of a mixed-fisheries effort-management system based on the}},
url = {https://watermark.silverchair.com/api/watermark?token=AQECAHi208BE49Ooan9kkhW{\_}Ercy7Dm3ZL{\_}9Cf3qfKAc485ysgAAAgkwggIFBgkqhkiG9w0BBwagggH2MIIB8gIBADCCAesGCSqGSIb3DQEHATAeBglghkgBZQMEAS4wEQQMNEQsqii2cXZB3QQWAgEQgIIBvPgHF3eB7B41wB25B2kJbf4Vv3OrtKuC{\_}W54QXzKH-IeI},
volume = {67},
