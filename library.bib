pmid = {23341621},
title = {{Income diversification and risk for fishermen.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3568353{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {110},
year = {2013}
}
