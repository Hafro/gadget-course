@article{Subbey2007,
abstract = {Quantifying fish stocks through modeling involves estimating time-varying parameters such as growth, stock recruitment, mortality rates and fishing levels. Each of these parameters vary non-uniformly and non-smoothly with time. For a given parameter, e.g., growth, the standard approach has been to estimate a single value of the parameter per time-step. When the modeling spans several time-steps this compounds the complexity of the model by increasing the dimension of the parameter space. This paper presents a method for reducing model complexity by effective reparameterization of selected time-varying parameters. The method involves representing the parameters by truncated (i.e., a finite) Fourier series, where the dimension of the Fourier coefficients is very much less than the number of time-steps in the modeling. Example results are presented, which show that the reparameterization can lead to over 40{\%} decrease in the number of model parameters associated with fishing levels, growth and stock recruitment. We illustrate using time-varying parameters associated with fishing levels, growth and stock recruitment, derived from an optimized biological model. The input data for the biological model is based on landed catch-data of Northeast Arctic cod (Gadus morhua). {\textcopyright} 2007 Elsevier B.V. All rights reserved.},
author = {Subbey, Sam and Howell, Daniel and Bogstad, Bjarte and {\AA}snes, Morten},
doi = {10.1016/j.fishres.2006.12.008},
file = {:C$\backslash$:/Users/bthe/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Subbey et al. - 2007 - Reducing fisheries model complexity using a finite Fourier series reparameterization.pdf:pdf},
isbn = {0165-7836},
issn = {01657836},
journal = {Fisheries Research},
keywords = {Fish stock,Fourier series,Model complexity,Parameter reduction},
number = {3},
pages = {390--394},
title = {{Reducing fisheries model complexity using a finite Fourier series reparameterization}},
volume = {84},
year = {2007}
}
