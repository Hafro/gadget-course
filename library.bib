@article{Clara2017,
abstract = {Achieving single species maximum sustainable yield (MSY) in complex and dynamic fisheries targeting multiple species (mixed fisheries) is challenging because achieving the objective for one species may mean missing the objective for another. The North Sea mixed fisheries are a representative example of an issue that is generic across most demersal fisheries worldwide, with the diversity of species and fisheries inducing numerous biological and technical interactions. Building on a rich knowledge base for the understanding and quantification of these interactions, new approaches have emerged. Recent paths towards operationalizing MSY at the regional scale have suggested the expansion of the concept into a desirable area of " pretty good yield " , implemented through a range around FMSY that would allow for more flexibility in management targets. This article investigates the potential of FMSY ranges to combine long-term single-stock targets with flexible, short-term, mixed-fisheries management requirements applied to the main North Sea demersal stocks. It is shown that sustained fishing at the upper bound of the range may lead to unacceptable risks when technical interactions occur. An objective method is suggested that provides an optimal set of fishing mortality within the range, minimizing the risk of total allowable catch mismatches among stocks captured within mixed fisheries, and addressing explicitly the trade-offs between the most and least productive stocks.},
author = {Clara, Ulrich and Youen, Vermard and Paul, Dolder J and Thomas, Brunel and Ernesto, Jardim and Steven, Holmes J and Alexander, Kempf and Lars, Mortensen O and Jan-Jaap, Poos and Anna, Rindorf},
doi = {10.1093/icesjms/fsw126},
journal = {ICES Journal of Marine Science: Journal du Conseil},
keywords = {Common Fisheries Policy,FMSY ranges,choke species,fleet modelling,landing obligation,management plan,pretty good yield},
number = {2},
pages = {566--575},
title = {{Achieving maximum sustainable yield in mixed fisheries: a management approach for the North Sea demersal fisheries}},
url = {http://dx.doi.org/10.1093/icesjms/fsw126 http://archimer.ifremer.fr/doc/00347/45834/},
volume = {74},
year = {2017}
}
@article{Johnson2000,
abstract = {The author demonstrates that for most of human history, life was both short and difficult for the majority of the world's people, that food supply was a major factor affecting population size, and that consumption was very limited. The author refers to Robert Malthus' famous book 'An Essay on the Principle of Population' (1798), which is most often credited with the pessimistic view that population growth carries with it the seeds of disaster for humanity and that population growth would inevitably press against the food supply. The author then looks at the question, what made it possible for the developed world to escape from what could be called the Malthusian trap during the nineteenth century? According to the author, the answer is the creation of knowledge. The author explains that there was an explosion of knowledge over the past two centuries that made possible an unparalleled increase in per capita well-being. New technologies have been developed at a rate unparalleled by historical standards. Three major factors are emphasized in the paper: the significant advances in agricultural productivity in the eighteenth and nineteenth centuries; the enormous increase in knowledge over the past two centuries; and the response of families to the removal of restraints on their well-being imposed by limited food supplies.},
author = {Johnson, D Gale},
issn = {00028282},
journal = {American Economic Review},
keywords = {1766-1834,Economic development,Essay on the Principle of Population: Text,Food supply,Malthus,Malthusianism,McGill - Resource,Population,Production (Economic theory),Source,T. R. (Thomas Robert)},
