@article{Foley2015,
abstract = {Critical analyses of neoliberalism[U+05F3]s influence on fisheries governance have documented how enclosure, quota leasing and renting, and commodification can precipitate negative social consequences for fishing communities. By contrast, this paper draws on the concept of embeddedness to argue that certain policies and social relations can regulate enclosure, quota renting, and commodification in ways that empower community-based groups to facilitate the anchoring of fishery resources and wealth in coastal communities. It does so through an analysis of northern shrimp fisheries in Newfoundland and Labrador, Canada, between the 1970s and the early 2000s. This case study illustrates how fisheries enclosure policies informed by geographically and morally defined principles of access and equity and limits on commodification can meaningfully embed fishery resources and benefits in rural and remote coastal regions that depend on small-scale fishing. Although the application of social principles continues to be marginalized in the context of neoliberal policy regimes that privilege individual economic efficiency over distributive concerns, this paper provides new insight into the conditions under which principles of ethical allocation and distribution of resources are able to persist through an era of neoliberalism.},
author = {Foley, Paul and Mather, Charles and Neis, Barbara},
doi = {10.1016/j.marpol.2014.11.009},
issn = {0308597X},
journal = {Marine Policy},
keywords = {Catch shares,Enclosure,Governance,Neoliberalism,Political ecology},
pages = {390--400},
publisher = {Elsevier Ltd},
title = {{Governing enclosure for coastal communities: Social embeddedness in a Canadian shrimp fishery}},
url = {http://linkinghub.elsevier.com/retrieve/pii/S0308597X14003030},
volume = {61},
year = {2015}
}
@article{Chetty2012,
abstract = {How can price elasticities be identified when agents face optimization frictions such as adjustment costs or inattention? I derive bounds on structural price elasticities that are a function of the observed effect of a price change on demand, the size of the price change, and the degree of frictions. The degree of frictions is measured by the utility losses agents tolerate to deviate from the frictionless optimum. The bounds imply that frictions affect intensive margin elasticities much more than extensive margin elasticities. I apply these bounds to the literature on labor supply. The utility costs of ignoring the tax changes used to identify intensive margin labor supply elasticities are typically less than 1{\%} of earnings.As a result, small frictions can explain the differences between micro and macro elasticities, extensive and intensive margin elasticities, and other disparate findings. Pooling estimates from existing studies, I estimate a Hicksian labor supply elasticity of 0.33 on the intensive margin and 0.25 on the extensive margin after accounting for frictions.},
author = {Chetty, Raj},
doi = {10.3982/ECTA9043},
isbn = {0012-9682},
issn = {0012-9682},
journal = {Econometrica},
keywords = {McGill - Labour},
