isbn = {0964-5691},
issn = {01657836},
journal = {Fisheries Research},
keywords = {Data conflict,Data weighting,Fisheries stock assessment,Model misspecification},
pages = {5--15},
publisher = {Elsevier B.V.},
title = {{Revisiting data weighting in fisheries stock assessment models}},
url = {http://dx.doi.org/10.1016/j.fishres.2016.06.006},
