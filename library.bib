@misc{Arnason1993a,
abstract = {This paper is concerned with ocean fisheries management with particular{\$}\backslash{\$}nemphasis on the application of economic theory to fisheries management{\$}\backslash{\$}npractice. The paper begins by briefly discussing the common property{\$}\backslash{\$}nnature of ocean fisheries and explains how the common property problem{\$}\backslash{\$}ngives rise to the need for a specific management of fisheries. It{\$}\backslash{\$}ngoes on to examine various proposed methods of fisheries management{\$}\backslash{\$}narguing that only property-rights-based fisheries management systems,{\$}\backslash{\$}nsuch as individual transferable quotas (ITQs) or fisheries management{\$}\backslash{\$}nbased on the imposition of the appropriate tax on catch seem to be{\$}\backslash{\$}ncapable of delivering the full potential economic benefits of ocean{\$}\backslash{\$}nfisheries. Finally, it reviews recent developments in the management{\$}\backslash{\$}nof actual ocean fisheries with special reference to Australia, Iceland{\$}\backslash{\$}nand New Zealand. The paper concludes that an economic rationalization{\$}\backslash{\$}nof ocean fisheries is probably historically inevitable. Moreover,{\$}\backslash{\$}njudging from historical trends in fisheries management, this rationalization{\$}\backslash{\$}nwill probably be accomplished by a property-rights-based management{\$}\backslash{\$}nsystem such as the ITQ system.},
