@article{Cairncross1989,
abstract = {Economic history occupies a disputed area between history and economic theory with their different approaches to past events. But the two disciplines need to join forces in order to arrive at a meaningful interpretation. The historian gains by asking questions prompted by economic theory while for the economist the record of the past is his only laboratory, in which the significance of the questions he raises can be put to the test. An historical perspective and an understanding of economic interactions are both indispensable in judging what matters and what does not in the performance of an economy. If, however, economic history is to thrive, the results of scholarly research must be supplemented by a deliberate effort of vulgarization for the benefit of the layman.},
author = {Cairncross, a. K},
doi = {10.2307/2596200},
issn = {00130117},
journal = {The Economic History Review},
number = {2},
