publisher = {American Association for the Advancement of Science},
title = {{Ecosystem-Based Fishery Management}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/11729316 http://www.ncbi.nlm.nih.gov/pubmed/15256658{\%}0Ahttp://www.ncbi.nlm.nih.gov/pubmed/15256658},
volume = {305},
year = {2004}
