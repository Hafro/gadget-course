@article{Daw2005,
abstract = {Within the European Union, scientific fisheries research and advice is passed down a chain of central European institutions before being incorporated into policy. However, advice is often poorly or only partially implemented and the system has largely failed to achieve sustainable fisheries management. This paper aims to explain why, by examining the use of fisheries science within the Common Fisheries Policy (CFP). The problem is examined with respect to both the political system and fisheries science, to show that the factors in both the supply, and the subsequent use, of scientific data inhibit effective fisheries management.},
