@article{Nøstbakken2012,
abstract = {I analyze drivers of investment in a fishery with tradable quotas.{\$}\backslash{\$}nTheory predicts that tradable quotas increase efficiency because{\$}\backslash{\$}nefficient firms buy quotas from less efficient firms. I identify{\$}\backslash{\$}nwhat drives investment, using data on Norwegian purse seiners. Results{\$}\backslash{\$}nshow that while the basic economic investment model explains investment{\$}\backslash{\$}nwithin firms, it does not explain differences between firms. These{\$}\backslash{\$}ndifferences are explained by other firm-specific factors such as{\$}\backslash{\$}ngeographical location, whether it is a family firm, and prices paid{\$}\backslash{\$}nfor quotas. Consequently, investment strategies are not necessarily{\$}\backslash{\$}ndriven by efficiency considerations, and quota trade may not increase{\$}\backslash{\$}nallocative efficiency. (JEL Q22, Q28)},
