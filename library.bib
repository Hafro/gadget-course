title = {{Migration behaviour of norwegian spring spawning herring when entering the cold front in the Norwegian Sea}},
url = {http://www.tandfonline.com/doi/abs/10.1080/00364827.1997.10413644},
volume = {82},
year = {1997}
}
@article{Kydland1977,
abstract = {Even if there is an agreed-upon, fixed social objective function and{\$}\backslash{\$}r{\$}\backslash{\$}n policymakers know the timing and magnitude of the effects of their{\$}\backslash{\$}r{\$}\backslash{\$}n actions, discretionary policy, namely, the selection of that decision which{\$}\backslash{\$}r{\$}\backslash{\$}n is best, given the current situation and a correct evaluation of the end-{\$}\backslash{\$}r{\$}\backslash{\$}n of-period position, does not result in the social objective function being{\$}\backslash{\$}r{\$}\backslash{\$}n maximized. The reason for this apparent paradox is that economic{\$}\backslash{\$}r{\$}\backslash{\$}n planning is not a game against nature but, rather, a game against{\$}\backslash{\$}r{\$}\backslash{\$}n rational economic agents. We conclude that there is no way control{\$}\backslash{\$}r{\$}\backslash{\$}n theory can be made applicable to economic planning when expectations{\$}\backslash{\$}r{\$}\backslash{\$}n are rational.},
