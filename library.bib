issn = {01657836},
journal = {Fisheries Research},
keywords = {Data weighting,Size-structured stock assessment methods,Tagging data},
number = {2015},
pages = {94--102},
publisher = {Elsevier B.V.},
title = {{Data weighting for tagging data in integrated size-structured models}},
url = {http://dx.doi.org/10.1016/j.fishres.2015.12.010},
volume = {192},
year = {2017}
}
