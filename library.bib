keywords = {18620,Abundance,Accuracy,Bias,Bootstrap,Calibration,Catch,Catches,Distribution,Method,Methods,Northwest Atlantic,Population analysis,Resources,SPA,Sequential population analysis,Stock assessment,Survey,Survey estimates,Trawl,Trawl survey,Uncertainty,atlantic,population},
