journal = {Proceedings of the National Academy of Sciences},
number = {15},
pages = {6067--6072},
pmid = {16608913},
title = {{Persistence of spatial populations depends on returning home}},
url = {http://www.pnas.org/cgi/doi/10.1073/pnas.0506651103},
volume = {103},
year = {2006}
}
