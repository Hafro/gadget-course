issn = {10543139},
journal = {ICES Journal of Marine Science},
keywords = {Herring,Minke whale,Northeast Atlantic,Predation},
number = {1},
pages = {58--70},
title = {{Predation on herring, Clupea harengus, by minke whales, Balaenoptera acutorostrata, in the Barents Sea}},
volume = {59},
year = {2002}
}
