@article{Grainger2013,
abstract = {Despite compelling evidence that property rights–based management can improve the economic and ecological performance of fisheries, reform proposals are often met with political opposition. Moreover, the opposition sometimes comes from incumbent fishermen and fishing communities that would ostensibly gain the most from rent-enhancing reforms. We flesh out this puzzle by describing the political economy of past and present institutional changes in North America such as bans on fish traps, limited-entry regulations, and individual transferable quotas (ITQs). Our review identifies patterns of distri-butional concerns surrounding property rights; many of the same groups that opposed limits on entry and fish traps also oppose ITQs. We provide examples of how political opposition to ITQs is mollified by modifications in program design, although these design compro-mises may have efficiency costs. We conclude by pointing to the need for more research on equity-efficiency trade-offs and for greater atten-tion to fishermen heterogeneity in economic analysis.},
author = {Grainger, Corbett A and Parker, Dominic P},
doi = {10.1146/annurev-resource-091912-151838},
journal = {Annu. Rev. Resour. Econ},
keywords = {ITQs,JEL codes,P48 Keywords political economy,Q22,Q28,distributional effects,fisheries,individual transferable quotas,property rights},
pages = {369--386},
title = {{The Political Economy of Fishery Reform}},
url = {www.annualreviews.org},
volume = {5},
year = {2013}
}
@misc{Anderson2011,
abstract = {We show that grandfathering fishing rights to local users or recognizing first possessions is more dynamically efficient than auctions of such rights. It is often argued that auctions allocate rights to the highest-valued users and thereby maximize resource rents. We counter that rents are not fixed in situ, but rather depend additionally upon the innovation, investment, and collective actions of fishers, who discover and enhance stocks and convert them into valuable goods and services. Our analysis shows how grandfathering increases rents by raising expected rates of return for investment, lowering the cost of capital, and providing incentives for collective action.},
author = {Anderson, Terry and Arnason, Ragnar and Libecap, Gary D},
booktitle = {Annual Review of Resource Economics},
