@article{Diewert2008,
abstract = {A number of theoretical results on estimating returns to scale, technical progress and monopolistic markups are derived when there are multiple outputs and inputs. The choice of value added versus gross output and problems that arise in aggregation across sectors of an economy are also considered. Using US data on manufacturing, evidence is found of increasing returns to scale across all levels of aggregation. Technical progress is typically found to be insignificant implying that economic growth has been driven by increasing returns to scale rather than technical progress. Such findings have important implications for the macroeconomic modeling of economic fluctuations.},
author = {Diewert, W Erwin and Fox, Kevin J},
doi = {10.1016/j.jeconom.2008.05.002},
