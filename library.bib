number = {5},
pages = {1093--1102},
publisher = {Elsevier},
title = {{Stakeholders' participation in the fisheries management decision-making process: Fishers' perceptions of participation}},
url = {http://dx.doi.org/10.1016/j.marpol.2010.03.009},
volume = {34},
year = {2010}
