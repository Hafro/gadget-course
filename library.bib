title = {{Selecting and assessing social objectives for Australian fisheries management}},
volume = {53},
year = {2015}
}
@article{Andersen2008,
abstract = { Salmon prices exhibit substantial volatility. An understanding of{\$}\backslash{\$}nthe structure of volatility is of great interest since this is a{\$}\backslash{\$}nmajor contributor to economic risk in the salmon industry. The volatility{\$}\backslash{\$}nprocess in salmon prices was analysed based on weekly price data{\$}\backslash{\$}nfrom 1995 to 2007. The Generalized Autoregressive Conditional Heteroskedasticity{\$}\backslash{\$}n(GARCH) model was used to test for volatility clustering and persistence{\$}\backslash{\$}nof volatility for prices. We find evidence for and discuss the degree{\$}\backslash{\$}nof persistence and reversion in salmon price volatility. Further,{\$}\backslash{\$}nwe find increased volatility in periods of high prices. For the industry{\$}\backslash{\$}nthis means that larger expected profits more often than not come{\$}\backslash{\$}nat a tradeoff of greater price risk.},
