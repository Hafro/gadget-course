isbn = {0960-3166},
issn = {09603166},
journal = {Reviews in Fish Biology and Fisheries},
keywords = {Arcto-boreal ecosystem,Fisheries,Multispecies modelling},
number = {1},
pages = {101--104},
title = {{Points of view: A framework for multispecies modelling of Arcto-boreal systems}},
volume = {8},
year = {1998}
}
